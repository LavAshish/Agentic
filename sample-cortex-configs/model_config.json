{
  "name": "dc-ea-vc-mcp-agent",
  "auth": {
    "owners": [
      "akshat.jhalani@lilly.com"
    ],
    "allow_access_to_reports_of": [],
    "owners_group": [],
    "access_groups": [],
    "access_aws_roles": [],
    "owners_aws_roles": [],
    "users": [],
    "private": true
  },
  "displayName": "dc-ea-vc-mcp-agent",
  "model_description": "An AI assistant with some capabilities capabilities.",
  "security_config": "",
  "chain": [
    {
      "chain_class": "agent-chain",
      "model_iteration": 1,
      "order": 1,
      "chain_params": {
        "max_turns": 7,
        "execution_type": "handoff",
        "supervisor": {
          "prompt": "You are a helpful AI assistant with access to various Veeva Vault Clinical tools. You also have access to some general tools.",
          "description": "Main assistant that can use tools to help users"
        },
        "resources": {
          "executables": [
            {
              "name": "greeter",
              "type": "tool-call",
              "description": "This tool is a general greeter tool. Use this to greet users when they start chatting/asking questions or introduce themselves."
            },
            {
              "name": "health_check",
              "type": "tool-call",
              "description": "This tool is a general downstream health check utility. Use this and return the response from this tool when the user asks for health analytics or anything else related to it."
            }
          ]
        }
      }
    }
  ],
  "model_versions": [
    {
      "model_class": "lilly-openai",
      "model_iteration": 7,
      "priority": 1,
      "reasoning_effort": null,
      "enable_thinking": false,
      "advanced_param_overrides": {}
    }
  ],
  "prompts": {
    "no_context": "default_no_context",
    "with_context": "default_with_context",
    "with_json_context": "default_with_json_context",
    "enhance_query": "default_enhance_query",
    "sql": "default_sql",
    "agent_tool": "default_agent_tool",
    "cortex_agent_tool_prompt_v2": "default_cortex_agent_tool_prompt_v2",
    "table_summary": "default_table_summary",
    "summary": "default_summary",
    "entity_extraction": "default_entity_extraction",
    "rewrite": "default_rewrite",
    "kg_triple_extraction": "default_kg_triple_extraction"
  },
  "toolkits": [
    "dc-ea-vc-mcp-server-toolkit"
  ],
  "allowed_tools_list": [
    "greeter",
    "retrieve_veeva_document",
    "health_check"
  ],
  "data": [],
  "max_response_token_size": null,
  "doc_relevence_threshold": 0.5,
  "hybrid_search": {
    "rrf_relevance_threshold": 0.012,
    "rrf_constant": 60,
    "hybrid_score_type": "average",
    "lexical_average_weight": 50
  },
  "agent_tool_max_iterations": null,
  "app_binding": "chatbuilder",
  "multimodal": false,
  "labels": {},
  "k_value": 20,
  "token_buffer_size": 1.2,
  "temperature": 0.7,
  "top_p": 1,
  "stop": null,
  "seed": null,
  "logprobs": false,
  "rerank": {
    "enabled": false,
    "model": "cohere-rerank-3.5",
    "top_n": 20
  },
  "context_cache_key": ""
}